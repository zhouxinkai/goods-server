  "scripts": {
    "prestart": "yarn run build",
    "start": "pm2 start scripts/pm2.json",
    "watch": "tsc -p src/server -w --pretty",
    "server": "cross-env NODE_ENV=development node --inspect=9092 dist/server/",
    "predev": "concurrently \"del -f ./dist/*\" \"tsc -p src/server\"",
    "dev": "concurrently --names \"TSC,NODE\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run watch\" \"npm run server\"",
    "build": "cross-env NODE_ENV=production node scripts/build",
    "lint.client": "tslint -p src/client/tsconfig.json -e *.d.ts -c tslint.json",
    "lint.server": "tslint -p src/server/tsconfig.json -e *.d.ts -c tslint.json",
    "lint.fix": "prettier --write \"./**/*.{js,tsx,ts}\"",
    "analyz": "NODE_ENV=production webpack --config config/webpack.prod.js"
  },
  "pre-commit": [
    "lint.client",
    "lint.server"
  ],
